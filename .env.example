# Supabase configuration
# Replace these with your actual Supabase credentials from your project dashboard
# After adding your real credentials, rename this file to .env
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Other environment variables
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_GEMINI_API_KEY=your_gemini_api_key
VITE_GEMINI_ENDPOINT=https://generativelanguage.googleapis.com/v1/models/gemini-1.5-pro:generateContent

# Google Vision API configuration
VITE_GOOGLE_VISION_API_KEY=your_google_vision_api_key

# AWS configuration for Amazon Textract
VITE_AWS_REGION=eu-west-1
VITE_AWS_ACCESS_KEY_ID=your_aws_access_key_id
VITE_AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Feature toggles
VITE_ENABLE_EXPENSES=false
